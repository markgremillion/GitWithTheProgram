
<!-- Main -->
<div id="main" class="box">

    <!-- Header -->
    <div id="header">

        <!-- Logotyp -->
        <h1 id="logo"><a href="./" title="utahFood">Utah Food<span></span></a></h1>
        <hr class="noscreen" />          

        <!-- Search -->
        <div id="search" class="noprint">
            <form action="" method="get">
                <fieldset><legend>Search</legend>
                    <label><span class="noscreen">Find:</span>
                    <span id="search-input-out"><input type="text" name="" id="search-input" size="30" /></span></label>
                    <input type="image" src="./assets/design/search_submit.gif" id="search-submit" value="OK" />
                </fieldset>
            </form>
        </div> <!-- /search -->
        <div id="sign">
            <button id="signIn" type="button" class="btn btn-default"> <a href="/login">Sign In</a></button>
        </div>

    </div> <!-- /header -->

