    <div id="page-in" class="box">

        <div id="strip" class="box noprint">

            <!-- Breadcrumbs -->
           
            
        </div> <!-- /strip -->

        <!-- Content -->
        <div id="content">

            <!-- Article -->
            {{#each restaurant}}
                <div class="article">
                    <h2 id="r1"><span><a href="#">{{this.name}}</a></span></h2>
                    <p class="info noprint">
                        <span class="date">{{this.address}}</span><span class="noscreen">,</span>
                        <span class="cat"><a href="#">{{this.typeOfFood}}</a></span><span class="noscreen">,</span>
                        <span class="user"><a href="#">{{this.rate}}</a></span><span class="noscreen">,</span>
                        <span class="comments"><a href="#">{{this.price}}</a></span>
                    </p>

                    <p>{{this.bio}}</p>

                    <p class="btn-more box noprint"><strong><a href="./r1.html">Continue</a></strong></p>
                </div> <!-- /article -->

                <hr class="noscreen" />
            {{/each}}

            
        </div> <!-- /content -->

        <!-- Right column -->
        <div id="col" class="noprint">
            <div id="col-in">

                <!-- About Me -->
